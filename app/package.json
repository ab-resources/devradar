{
  "name": "devradar",
  "private": true,
  "description": "Competence management for developers",
  "author": "Andreas Offenhaeuser <https://anoff.io>",
  "scripts": {
    "build": "vue-cli-service build",
    "cypress:ci": "cypress run --record",
    "cypress:ci:firebase": "cypress run --record --group firebase",
    "cypress:ci:localstorage": "cypress run --record --group localstorage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "vue-cli-service lint",
    "serve": "vue-cli-service serve",
    "serve:static": "serve dist --single --no-clipboard --listen 8080",
    "serve:test": "vue-cli-service serve --mode test",
    "start": "npm run serve",
    "test": "npm run lint && npm run build",
    "test:e2e:ci": "run-p --race serve:static cypress:ci",
    "test:e2e:ci:firebase": "run-p --race serve:static cypress:ci:firebase",
    "test:e2e:ci:localstorage": "run-p --race serve:static cypress:ci:localstorage"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.3",
    "@types/d3": "^5.7.2",
    "@types/uuid": "^3.4.6",
    "axios": "^0.19.1",
    "clipboard-copy": "^3.1.0",
    "core-js": "^3.6.2",
    "d3": "^5.15.0",
    "firebase": "^7.6.1",
    "markdown-it": "^10.0.0",
    "uuid": "^3.3.3",
    "vue": "^2.6.11",
    "vue-analytics": "^5.21.1",
    "vue-class-component": "^7.1.0",
    "vue-cookie-law": "^1.11.0",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vuetify": "^2.2.2",
    "vuetify-upload-button": "^2.0.2",
    "vuex": "^3.1.2",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@mdi/font": "^4.7.95",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-typescript": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "cypress": "^3.8.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "firebase-admin": "^8.9.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.24.2",
    "sass-loader": "^8.0.0",
    "serve": "^11.3.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "typescript": ">=3.2.1 <3.6.0",
    "vue-template-compiler": "^2.6.11"
  },
  "config": {
    "cz-conventional-kawaii": {
      "scopes": [
        "app"
      ],
      "quick": false
    }
  },
  "license": "MIT"
}
